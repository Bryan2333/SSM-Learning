<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.bryan02.three.AnotherBean" id="anotherBean"/>

    <bean class="com.bryan02.three.Bean" id="bean" c:anotherBean-ref="anotherBean" c:string="Test"/>

    <bean class="com.bryan02.three.Bean" id="bean2" p:anotherBean-ref="anotherBean" p:string="Test2"/>
</beans>
