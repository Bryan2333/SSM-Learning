<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="beanOne" class="com.bryan02.two.BeanOne"/>

    <bean id="beanTwo" class="com.bryan02.two.BeanTwoFactory" factory-method="getBeanTwo"/>

    <bean id="beanThreeFactory" class="com.bryan02.two.BeanThreeFactory"/>

    <bean id="beanThree" class="com.bryan02.two.BeanThree" factory-bean="beanThreeFactory" factory-method="getBeanThree"/>

    <bean id="anotherBean" class="com.bryan02.three.AnotherBean"/>

    <bean id="bean1" class="com.bryan02.three.Bean">
        <constructor-arg index="0" name="anotherBean" type="com.bryan02.three.AnotherBean" ref="anotherBean"/>
        <constructor-arg index="1" name="string" type="java.lang.String" value="Test"/>
    </bean>
</beans>
