<?xml version="1.0" encoding="UTF-8"?>
<!-- log4j2 日志配置文件 -->
<Configuration>
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%5p[%t] - %m%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="DEBUG">
            <AppenderRef ref="Console"/>
        </Root>

        <Logger name="org.mybatis" level="DEBUG"/>
    </Loggers>
</Configuration>
